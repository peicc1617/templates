        <script>
            let PROJECT_USERNAME = new Set();
            $(document).ready(function () {

                $('#user-new-modal').on('show.bs.modal',
                    function() {
                        $("#user-list-table").bootstrapTable('refresh');

                    })
                $("#project-role-table").bootstrapTable({
                    columns: [{
                        field: 'id',
                        visible: false
                    }, {
                        field: 'username',
                        title: '用户名',
                        formatter:function (value) {
                            PROJECT_USERNAME.add(value);
                            return value;
                        }
                    }, {
                        field: 'projectRole',
                        title: '项目内角色',
                        editable: {
                            type: 'select',
                            title: '修改项目内的角色',
                            source: function () {
                                var result = [];
                                $.ajax({
                                    url: '/templates/api/project/role/map',
                                    async: false,
                                    type: "get",
                                    success: function (data, status) {
                                        for (let text in data) {
                                            result.push({value: data[text], text: text});
                                        }

                                    }
                                });
                                return result;
                            }
                        }
                    }, {
                        field: 'memberRole',
                        title: '是否可以管理用户',
                        editable: {
                            type: 'select',
                            title: '部门',
                            source: [{value: 1, text: '是'}, {value: 0, text: '否'}]
                        }
                    },{
                        field:'op',
                        title:'删除用户',
                        formatter:function (value,row,index) {
                            $button = $("<button></button>").attr({
                                'class': 'btn btn-xs btn-danger',
                                'onclick':'removeFromProject("'+row.username+'")'
                            });
                            return $button.append('<i class="ace-icon fa fa-trash-o bigger-120"></i>')[0].outerHTML;
                        }
                    }],
                    url: "/templates/api/project/role?projectID=" + PROJECT_ID,
                    method: "GET",
                    onEditableSave(field, row, oldValue, $el) {
                        let url = "/templates/api/project/role";
                        if (field == "memberRole") {
                            url += "/member";
                        }
                        let data = {
                            projectID: PROJECT_ID,
                            username: row.username,
                        }
                        data[field] = row[field];
                        $.ajax({
                            url: url,
                            method: "PUT",
                            data: data,
                            async: false,
                            success: function (data) {
                                alert("修改成功")
                            },
                            error: function () {
                                row[field] = oldValue;
                                alert("修改失败")
                            },
                        })
                    }
                })
                $("#user-list-table").bootstrapTable();
            });

            function opFormatter(value, row, index) {
                const username = row.username;
                $button = $("<a></a>").attr({
                    username: row.username,
                });
                if (!PROJECT_USERNAME.has(username)) {

                    $button.attr({
                        'class': 'blue',
                        'onclick':'addToProject("'+row.username+'")'
                    }).append("<i class=\"ace-icon glyphicon glyphicon-plus bigger-120\"></i>")
                    $div = $("<div></div>").addClass("action-buttons").append($button);
                    return $div[0].outerHTML;
                }
            }

            function addToProject(username) {
                $.ajax({
                    url: '/templates/api/project/role',
                    'type': 'POST',
                    'data': {
                        projectID: PROJECT_ID,
                        username: username,
                        projectRole: 3,
                        memberRole: 0
                    },
                    success: function (data) {
                        alert("添加成功")
                        PROJECT_USERNAME.add(username);
                        $("#project-role-table").bootstrapTable('refresh')
                        $("#user-list-table").bootstrapTable("refresh");
                    }
                })
            }

            function removeFromProject (username) {
                $.ajax({
                    url: '/templates/api/project/role',
                    'type': 'DELETE',
                    'data': {
                        projectID: PROJECT_ID,
                        username: username,
                    },
                    success: function (data) {
                        alert("删除成功")
                        PROJECT_USERNAME.delete(username);
                        $("#project-role-table").bootstrapTable('refresh')
                    }
                })
            }
